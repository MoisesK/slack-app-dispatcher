{
    "name": "moises-kalebe/slack-dispatcher-php",
    "description": "Simples publicador de mensagens no slack",
    "type": "library",
    "license": "GPL-3.0-or-later",
    "authors":[
        {
            "name": "Moises Kalebe",
            "email": "moises979@gmail.com"
        }
    ],
    "scripts": {
        "test:unit": "phpunit --do-not-cache-result --configuration tests/phpunit.xml --testdox --testsuite unit"
    },
    "autoload": {
        "psr-4": {
            "MoisesK\\SlackDispatcherPHP\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=8.1",
        "ext-curl": "*",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.10",
        "fakerphp/faker": "^1.16",
        "dg/bypass-finals": "^1.5"
    }
}
